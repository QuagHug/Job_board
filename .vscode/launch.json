{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/dist/app.js",
        "outFiles": [
          "${workspaceFolder}/dist/app.js"
        ],
        // "env": {
        //   "NODE_ENV": "development"
        // },
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ],
        // "preLaunchTask": "prepare app to debug",
        // "postDebugTask": "clean app after debug"
      }
    ]
  }